<h1>Login</h1>

<%= form_with url: sessions_path, local: true do |f| %>
    <div>
        <%= f.label :email %>
        <%= f.email_field :email%>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>

    <%= f.submit "Login", class: "btn btn-primary" %>
<% end %>