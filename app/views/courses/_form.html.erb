<% if @course.errors.any? %>
  <ul>
      <% @course.errors.full_messages.each do |msg|%>
      <li><%= msg%></li>
      <% end %>
  </ul>
<% end %>
<%= form_for @course do |f| %>
  <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
  </div>
  <div>
      <%= f.label :session %>
      <%= f.text_field :session %>
  </div>
  <div>
      <%= f.label :slack_group %>
      <%= f.text_field :slack_group %>
  </div>
  <div>
      <%= f.label :start %>
      <%= f.date_field :start %>
  </div>
  <div>
      <%= f.label :end %>
      <%= f.date_field :end %>
  </div>
  <div>
      <%= f.label :picture_url %>
      <%= f.text_field :picture_url %>
  </div>
  <div>
      <%= f.label :program_id %>
      <%= f.collection_select :program_id, @programs, :id, :name, include_blank: true %>
  </div>
    <div>
      <%= f.label :status_id %>
      <%= f.collection_select :status_id, @statuses, :id, :status, include_blank: true %>
  </div>
<%= f.submit :submit %>
<% end %> 